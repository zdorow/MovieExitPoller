<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.worldcom.movieexitpoller">
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:ignore="GoogleAppIndexingWarning">
        <activity
            android:name=".Activities.MainActivity"
            android:label="@string/app_name">
            <intent-filter>
            <action android:name="android.intent.action.MAIN" />
            <category android:name="android.intent.category.LAUNCHER" />
        </intent-filter></activity>
        <activity
            android:name=".Activities.SurveyActivity"
            android:label="Exit Poll">
        </activity>
        <activity
            android:name=".Activities.StatsActivity1"
            android:label="Exit Poll Stats" />
        <activity
            android:name=".Activities.StatsActivity2"
            android:label="Exit Poll Stats" />
        <activity
            android:name=".Activities.StatsActivity3"
            android:label="Exit Poll Stats" />
        <activity
            android:name=".Activities.StatsActivity4"
            android:label="Exit Poll Stats" />
        <activity
            android:name=".Activities.StatsActivity5"
            android:label="Exit Poll Stats" />
        <activity
            android:name=".Activities.StatsActivity6"
            android:label="Exit Poll Stats" />
    </application>

</manifest>